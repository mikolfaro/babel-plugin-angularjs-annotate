{
  "name": "babel-plugin-angularjs-annotate",
  "version": "0.8.2",
  "description": "Babel plugin to add angularjs dependency injection annotations",
  "main": "babel-ng-annotate.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/schmod/babel-plugin-angularjs-annotate"
  },
  "dependencies": {
    "babel-code-frame": "^6.26.0",
    "babel-types": "^6.26.0",
    "simple-is": "~0.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chalk": "^1.1.1",
    "diff": "^2.2.3",
    "eslint": "^4.11.0",
    "indent-string": "^2.1.0",
    "tap-xunit": "^1.7.0",
    "tape": "^4.8.0",
    "watchify": "^3.9.0"
  },
  "keywords": [
    "angular",
    "angularjs",
    "di",
